# DataForSEO credentials
DATAFORSEO_USERNAME=your_username
DATAFORSEO_PASSWORD=your_password

# Neon Postgres (via Vercel Storage)
DATABASE_URL=postgresql://user:password@host/neondb?sslmode=require

# x402 Payment Protocol
EVM_ADDRESS=0x...                              # Your wallet address for receiving USDC on Base

# Coinbase Developer Platform (https://portal.cdp.coinbase.com/)
CDP_API_KEY_ID=your_cdp_api_key_id             # Required for x402 facilitator auth
CDP_API_KEY_SECRET=your_cdp_api_key_secret     # Required for x402 facilitator auth
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key    # Required for OnchainKit wallet UI


# Privy Authentication (https://dashboard.privy.io)
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id     # Public Privy app ID
PRIVY_APP_SECRET=your_privy_app_secret         # Secret for server-side token verification
PRIVY_VERIFICATION_KEY=your_verification_key   # JWT verification key for optimized token validation

# Upstash Redis (https://console.upstash.com) - for distributed rate limiting
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token
